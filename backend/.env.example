PORT=5000

# Database Configuration
# Make sure PostgreSQL is installed and running
# Create a database called 'quizmaster' before running the app
# 1. Install PostgreSQL if not already installed
# 2. Run: createdb quizmaster
# 3. Or connect to PostgreSQL and run: CREATE DATABASE quizmaster;
DB_HOST=localhost
DB_PORT=5432
DB_NAME=quizmaster
DB_USER=postgres
DB_PASSWORD=postgres

# JWT Configuration 
JWT_SECRET=quizmaster_secret_key
JWT_EXPIRES_IN=7d 